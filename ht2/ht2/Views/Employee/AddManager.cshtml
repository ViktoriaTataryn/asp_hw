@model Manager

<form method="post" >
    <label>Name: </label>
    <input type="text" asp-for="Name" required />
    <br />
    <br />
    <label>Surname: </label>
    <input type="text" asp-for="Surname" required />
    <br />
    <br />
    <label>Email: </label>
    <input type="text" asp-for="Email" required />
    <br />
    <br />
    <label>Salary: </label>
    <input type="number" asp-for="Salary" required min="1" />
    <br />
    <br />
    <label>Employee Count: </label>
    <input type="number" asp-for="EmployeeCount" required min="1" />
    <br />
    <br />
    <input type="submit" />

</form>

